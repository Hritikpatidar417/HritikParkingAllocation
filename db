CREATE TABLE ParkingHistory (
    sno INT PRIMARY KEY ,
    parkingSlot INT,
    employeeId INT,
    employeeName VARCHAR(255),
    date DATE,
    vehicleNo VARCHAR(255),
    startTime TIME,
    endTime TIME
);

CREATE TABLE Parking (
    parkingId INT PRIMARY KEY ,
    userId INT,
    parkingHistoryId INT,
    userName VARCHAR(255),
    status VARCHAR(255),
    vehicleNo VARCHAR(255),
    parkingType VARCHAR(255)
);

CREATE TABLE Users (
    userId INT PRIMARY KEY,
    name VARCHAR(255),
    role VARCHAR(255),
    password VARCHAR(255)
);